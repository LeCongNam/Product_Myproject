{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "ignorePatterns": [
        "**/node_modules/**",
        "package.json",
        "package-lock.json"
    ],
    "rules": {
        "no-console": "error",
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    }
}